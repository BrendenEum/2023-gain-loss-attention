	file = {Cole et al. - 2019 - Task activations produce spurious but systematic i.pdf:D\:\\Program Files\\Zotero\\data-directory\\storage\\EXN9WETV\\Cole et al. - 2019 - Task activations produce spurious but systematic i.pdf:application/pdf},
}

