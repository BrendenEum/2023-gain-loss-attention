@article{fung_slow_2019,
	title = {Slow, but not fast, escape decisions are swayed by trait anxiety},
	volume = {3},
	doi = {10.1038/s41562-019-0595-5},
	abstract = {Theoretical models distinguish between neural responses elicited by distal threats and those evoked by more immediate threats1–3. Speciﬁcally, slower, “cognitive” fear responses involve a network of brain regions including ventral hippocampus (vHPC) and medial prefrontal cortex (mPFC), while immediate, “reactive” fear defensive responses rely on regions such as the periaqueductal gray (PAG)4, 5. It is unclear, however, how anxiety and its neural substrates relate to these distinct defensive survival circuits. We tested whether individual differences in trait anxiety would impact escape behavior and neural responses to slow and fast attacking predators: conditions designed to evoke “cognitive” and “reactive” fear, respectively. Behaviorally, we found that trait anxiety was not related to escape decisions for fast threats, but individuals with higher trait anxiety escaped earlier during slow threats. Functional MRI showed that when subjects faced slow threats, trait anxiety positively correlated with activity in the vHPC, mPFC, amygdala and insula. Further, the strength of the functional coupling between the vHPC and mPFC was correlated with the degree of trait anxiety. These ﬁndings suggest that anxiety plays little or no role in escape under conditions of proximal threat. Instead, anxiety affects “cognitive” fear circuits that are involved in volitional strategic escape.},
	language = {en},
	journal = {Nature Human Behaviour},
	author = {Fung, Bowen J and Qi, Song and Hassabis, Demis and Daw, Nathaniel and Mobbs, Dean},
	year = {2019},
	pages = {702--708},
	file = {Fung et al. - Slow, but not fast, escape decisions are swayed by.pdf:D\:\\Program Files\\Zotero\\data-directory\\storage\\8C3HSXXV\\Fung et al. - Slow, but not fast, escape decisions are swayed by.pdf:application/pdf},
}

