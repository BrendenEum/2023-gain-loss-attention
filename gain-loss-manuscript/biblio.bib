}

@article{acharya_rational_nodate,
	title = {Rational {Inattention} in {Hiring} {Decisions}},
	abstract = {This paper provides an information-based theory of match eﬃciency. Rationally inattentive hiring ﬁrms must expend resources to determine the viability of hiring an applicant. In a recession, ﬁrms are more selective and seek to hire and retain more productive workers to compensate for lower aggregate productivity. Stricter standards increase the variability in the pool of unemployed job-seekers, making it harder and costlier for the ﬁrm to ascertain the suitability of an applicant. These higher screening costs limit the ﬁrms’ ability to effectively screen applicants, leading them to accept fewer applicants in order to avoid hiring unsuitable workers. These pro-cyclical acceptance rates form a wedge between meeting and hiring rates and corresponds to changes in match eﬃciency. Unlike the standard search model which generates counterfactual predictions, our model with rationally inattentive ﬁrms can account for ﬂuctuations in measured match eﬃciency in the data.},
	language = {en},
	author = {Acharya, Sushant and Wee, Shu Lin},
	pages = {42},
}

